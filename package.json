{
  "name": "payload-nextjs-server",
  "description": "A boilerplate project for Next.js with Payload",
  "keywords": ["payload", "cms", "nextjs"],
  "engines": {
    "npm": ">=9.0.0 <10.2.5",
    "node": ">=18.18.12 <20.9.0"
  },
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "author": "TRBL",
  "scripts": {
  "build:next": "npm run build:payload && npm run build:server && npm run copyfiles:media && next build",
  "build:server": "tsc --project tsconfig.server.json",
  "build:payload": "payload build",
  "copyfiles:media": "copyfiles -u 1 media/**/*.{svg,jpg,png} dist/media",
  "build": "npm run build:next",
  "dev": "ts-node server.ts",
  "seed": "ts-node seed/index.js",
  "serve": "cross-env NODE_ENV=production PAYLOAD_CONFIG_PATH=dist/payload.config.js node dist/server.js",
  "tailwind": "tailwindcss -i ./css/tailwind.css -o ./css/compiledTailwind.css --watch"
},
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.3",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.0",
    "escape-html": "^1.0.3",
    "express": "^4.18.0",
    "lucide-react": "^0.300.0",
    "next": "^13.2.3",
    "payload": "^2.5.0",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive-carousel": "^3.2.23",
    "sass": "^1.52.0",
    "slate": "^0.72.8",
    "swiper": "^11.0.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^4.7.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@types/react": "^16.9.56",
    "@typescript-eslint/parser": "^4.0.1",
    "daisyui": "^4.4.23",
    "cross-env": "^7.0.3",
    "ts-node": "^10.8.0"
  }
}
